ARRAY MEAN
  
#include <stdio.h>

int main() {
    int n;
    scanf("%d", &n);  // Read the number of elements

    int arr[n];
    int sum = 0;

    for(int i = 0; i < n; i++) {
        scanf("%d", &arr[i]);  // Read each element
        sum += arr[i];         // Add to sum
    }

    float mean = (float)sum / n;
    printf("Array Mean Value is %.2f\n", mean);

    return 0;
}
