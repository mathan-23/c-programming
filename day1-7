Value Performance
#include <math.h>
#include <stdio.h>
#include <string.h>
#include <stdlib.h>
#include <assert.h>
#include <limits.h>
#include <stdbool.h>

int main() {
    int a;
    float b;
    scanf("%d",&a);
    printf("%d\n",a);
    scanf("%f",&b);
    printf("%.2f\n",b);
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */
    return 0;
}
