#include <stdio.h>
#include <math.h>
#include <stdbool.h>

(int n) {
    if (n <= 1) return false;
    for (int i = 2; i <= sqrt(n); i++)
        if (n % i == 0) return false;
    return true;
}

int main() {
    int m;
    scanf("%d", &m);

    if (m >= 7) {
        printf("Enter the valid Input\n");
        return 0;
    }

    int matrix[m][m];

    for (int i = 0; i < m; i++)
        for (int j = 0; j < m; j++)
            scanf("%d", &matrix[i][j]);

    printf("The Prime Array Matrix is:\n");
    for (int i = 0; i < m; i++) {
        for (int j = 0; j < m; j++) {
            if (isPrime(matrix[i][j]))
                printf("T ");
            else
                printf("F ");
        }
        printf("\n");
    }

    return 0;
}
