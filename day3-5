
  #include <stdio.h>

int main() {
    int n;
    scanf("%d", &n);

    int arr[n];
    double product = 1.0, sum = 0.0;

    for (int i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
        product *= arr[i];
        sum += arr[i];
    }

    double mean = sum / n;

    printf("Product of the array:%.2lf\n", product);
    printf("Mean of the array:%.3lf\n", (int)(mean * 1000) / 1000.0);

    return 0;
}
