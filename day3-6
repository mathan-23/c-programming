Search an Element
#include <stdio.h>

int main() {
    int n, key, found = 0;
    scanf("%d", &n);  // Read the size of the array

    int arr[n];
    for (int i = 0; i < n; i++) {
        scanf("%d", &arr[i]);  // Read each element
    }

    scanf("%d", &key);  // Element to search for

    for (int i = 0; i < n; i++) {
        if (arr[i] == key) {
            found = 1;
            break;
        }
    }

    if (found)
        printf("%d is presented in an array.\n", key);
    else
        printf("%d is not presented in an array.\n", key);

    return 0;
}
