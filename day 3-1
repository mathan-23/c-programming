Same or Not
  

#include <math.h>
#include <stdio.h>
#include <string.h>
#include <stdlib.h>
#include <assert.h>
#include <limits.h>
#include <stdbool.h>

int main() 
{
    int a[100],n;
    int cnt1=0,cnt2=0;
    scanf("%d",&n);
    for(int i=0;i<n;i++)
    {

    scanf("%d",&a[i]);
    }
    for(int i=0;i<n;i++)
    {

      
        if(a[i]%2==0)
        {
          cnt1++;
        }
        else if(a[i]%2!=0)
            cnt2++;
        
    }
    if(n==cnt1)
        printf("Even");
    else if(n==cnt2)
        printf("Odd");
    else
        printf("Mixed");
    return 0;
}
