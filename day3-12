Concatenation Of Array
#include <stdio.h>

int main() {
    int n;
    scanf("%d", &n);  // Array size

    int arr[n];
    for (int i = 0; i < n; i++) {
        scanf("%d", &arr[i]);  // Read original elements
    }

    // Print concatenated array (2n elements)
    for (int i = 0; i < 2 * n; i++) {
        printf("%d ", arr[i % n]);  // Repeat elements using modulo
    }

    printf("\n");
    return 0;
}
