Best Leader
#include <stdio.h>

int main() {
    int n;
    scanf("%d", &n);

    int arr[n], best[n];
    int count = 0;

    // Read scores
    for (int i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
    }

    // Traverse from right and collect all best leaders
    int currentMax = arr[n - 1];
    best[count++] = currentMax;

    for (int i = n - 2; i >= 0; i--) {
        if (arr[i] > currentMax) {
            currentMax = arr[i];
            best[count++] = currentMax;
        }
    }

    // Print leaders in reverse (to maintain left-to-right output)
    printf("The Best Leaders are");
    for (int i = 0; i < count; i++) {
        printf(" %d", best[i]);
    }
    printf("\n");

    return 0;
}
