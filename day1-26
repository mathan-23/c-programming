Shape Calculation-Perimeter
#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
//2*l+w//2*pi*r//sa+sb+sc;
//4*side
    int s,l,w,r,b1,b2,sc;
    float pi=3.141;
    scanf("%d %d %d %d %d %d %d",&s,&l,&w,&r,&b1,&b2,&sc);
    printf("Perimeter of Square:%d\n",s*4);
    printf("Perimeter of Rectangle:%d\n",2*(l+w));
    printf("Perimeter of Circle:%.2f\n",2*pi*r);
    printf("Perimeter of Triangle:%d\n",b1+b2+sc);
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
