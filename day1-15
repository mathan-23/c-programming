The newspaper agency
  #include <math.h>
#include <stdio.h>
#include <string.h>
#include <stdlib.h>
#include <assert.h>
#include <limits.h>
#include <stdbool.h>

int main() {
    int w,x,y,p;
    scanf("%d %d %d",&w,&x,&y);
    p=((x*w)-(y*w))-100;
    printf("%d",p);
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */
    return 0;
}
