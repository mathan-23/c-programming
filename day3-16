Transpose Matrix
#include <stdio.h>

int main() {
    int n;
    scanf("%d", &n);
    int matrix[n][n];

    // Reading matrix elements
    for (int i = 0; i < n * n; i++) {
        scanf("%d", &matrix[i / n][i % n]);
    }

    // Printing original matrix
    printf("Array elements are:\n");
    for (int i = 0; i < n; i++) {
        for (int j = 0; j < n; j++) {
            printf("%d ", matrix[i][j]);
        }
        printf("\n");
    }

    // Printing transpose
    printf("Transpose matrix is:\n");
    for (int i = 0; i < n; i++) {
        for (int j = 0; j < n; j++) {
            printf("%d ", matrix[j][i]);
        }
        printf("\n");
    }

    return 0;
}
