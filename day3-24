#include <stdio.h>
#include <string.h>

int main() {
    int n;
    scanf("%d", &n);

    int arr[n];
    for (int i = 0; i < n; i++)
        scanf("%d", &arr[i]);

    int count = 0;

    for (int i = 0; i < n; i++) {
    Distinct = true;
        for (int j = 0; j < i; j++) {
            if (arr[i] == arr[j]) {
                Distinct = false;
                break;
            }
        }
        if (isDistinct)
            count++;
    }

    if (count == 1)
        printf("There are 1 distinct element in the array.\n");
    else
        printf("There are %d distinct elements in the array.\n", count);

    return 0;
}
